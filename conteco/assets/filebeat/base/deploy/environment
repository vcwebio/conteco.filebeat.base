#!/usr/bin/env bash

# global replacements must be done to all templates
# local replacements are only for this instance

# GLOBAL REPLACEMENTS
DOLLAR='$'
export CONTECO_DC_GLOBAL='$CONTECO_DC_GLOBAL_SERVICE_NAME,$CONTECO_DC_GLOBAL_IMAGE_NAME'
export CONTECO_DC_GLOBAL_SERVICE_NAME="${CONTECO_TYPE}"
export CONTECO_DC_GLOBAL_IMAGE_NAME="${CONTECO_TYPE}.${CONTECO_NAME}:${CONTECO_TAG}"

# START VARIABLES filebeat.base
export CONTECO_DC_FILEBEAT_ENVIRONMENT=
export CONTECO_DC_FILEBEAT_COMMAND=
export CONTECO_DC_FILEBEAT_VOLUMES=
export CONTECO_DC_FILEBEAT_PORTS=
export CONTECO_DC_FILEBEAT_NETWORKS="${CONTECO_ECOSYSTEM}_overlay"
export CONTECO_DC_FILEBEAT_DEPLOY=
# END VARIABLES filebeat.base
